{
  "manifest_version": 3,
  "name": "String to CSV converter",
  "version": "1.0",
  "description": "Converts String into CSV",
  "action": {
    "default_popup": "index.html"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["scripts.js"]
    }
  ],
  "commands": {
    "_execute_action": {
      "suggested_key": {
        "default": "Ctrl+Shift+D",
        "mac": "Command+Shift+D"
      },
      "description": "Opens String to CSV converter"
    }
  }
}
