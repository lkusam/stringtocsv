{
  "manifest_version": 3,
  "name": "String to CSV converter",
  "version": "2.5",
  "description": "Converts String into CSV",
  "action": {
    "default_popup": "index.html"
  },
  "permissions": ["storage"],
  "commands": {
    "_execute_action": {
      "suggested_key": {
        "default": "Ctrl+Shift+D",
        "mac": "Command+Shift+D"
      },
      "description": "Opens String to CSV converter"
    }
  }
}
